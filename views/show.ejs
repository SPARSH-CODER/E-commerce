<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        *{
            margin: 0;
            padding: 0;

        }
        .navbar{
            background-color: blue;
            display: flex;
            flex-wrap: wrap;
            color: white;
            width: 100%;
            height: 70px;
            align-content: center;
            
        }
        .navbar h3{
            position: relative;
            left: 7%;
            font-size: 35px;
        }

        .navbar h6:nth-of-type(1){
            position: relative;
            left: 9%;
            top: 13px;
            font-size: 20px;
        }
        .navbar h6 a{
            color: white;
            text-decoration: none;
        }

        .navbar h6:nth-of-type(2){
            position: relative;
            left: 12%;
            top: 13px;
            font-size: 20px;

        }

        .navbar h6:nth-of-type(3){
            position: relative;
            left: 15%;
            top: 13px;
            font-size: 20px;

        }
        .navbar h6:nth-of-type(4){
            position: relative;
            left: 60vw;
            top: 13px;
            font-size: 20px;

        }
        h6:hover{
            color: rgb(158, 153, 153);
        }
        .innerdiv{
            height: 420px;
            width: 310px;
            display: flex;
            flex-wrap: wrap;
            border: 1px solid black;
        }
        
        .innerdiv{
            width: 320px;
            height: 450px;
            border: 2px solid black;
            margin: 30px;
            position: relative;
            left: 10%;
        }
         .innerdiv img{
            width: 320px;
            height: 200px;

         }
         .innerdiv h2{
            margin: 0px 200px 0 10px;
         }
         .innerdiv h3{
            margin: 0 0 0px 10px;         
        }
        a{
            margin: 0 0 0 10px;
            color: white;
            background-color: blue;
            text-decoration: none;
            font-size: 20px;
            height: 25px;
            padding: 1px 1px ;
            border-radius:10% ;
           
        }
        boby{
            position: relative;
        }
        .feedback{
            position: relative;
            left: 30%;
            width: 400px;
            top: 30px;
        }
        .out{
            display: flex;
            flex-wrap: wrap;
        }
        i{
            font-size: 40px;
        }

        
        label{
            font-size: x-large;
        }
        input{
            font-size: large;
            height: 100px;
            width: 500px;
            margin: 7px 0;
        }
        button{
            margin: 0 0 0 10px;
            color: white;
            background-color: blue;
            text-decoration: none;
            font-size: 20px;
            height: 27px;
            padding: 1px 1px ;
            border-radius:10% ;
        }
        .spe:hover{
            color: rgb(158, 153, 153);
        }
        
        textarea{
          margin: 7px 0;
        }

        
    </style>
</head>
<body>
    <nav class="navbar">
        <h3>E-Commerce</h3>
        <h6>Home</h6>
        <h6>Products</h6>
        <h6><a class="spe" href="/user/new">New</a></h6>
        <h6>Login</h6>
    </nav>
    <div class="out">
    
        <div class="innerdiv">
            <img src='<%= showdata.image %>' alt="a">
            <h2> <%= showdata.name %></h2> <br> 
            <h3> $ <%= showdata.prize %></h3>
            <h3> <%= showdata.description %></h3>
            <a href="#" style="background-color: red;">Buy now</a>
            <a href="#" style="background-color: green;">Add To Cart</a>
            <a href="/users/<%= showdata.id %>/edit" style="background-color: aqua;">Edit</a>
            <form action="/users/<%= showdata.id %>?_method=DELETE" method ="POST" >
                <button>Delete</button>
            </form>
            
        </div>
    
    <div class="feedback">
      <div>
        <h1>Leave Your Review</h1>
        <div class="stardiv">
            <i class="newstar1">&#9733</i>
            <i class="newstar2">&#9733</i>
            <i class="newstar3">&#9733</i>
            <i class="newstar4">&#9733</i>
            <i class="newstar5">&#9733</i>
        </div>
        <br>
        <form action="">
            <label for="description" >Description -</label> <br>
            <textarea name="description" id="des" cols="65" rows="7"></textarea>
            <button class="button" style="background-color: green;">submit</button>
        </form>
      </div>
      <br>
      <div class="put" style="width: 500px;">

      </div>
    </div>
</div>

<script>

let star1=document.querySelector('.stardiv .newstar1');
let star2=document.querySelector('.stardiv .newstar2');
let star3=document.querySelector('.stardiv .newstar3');
let star4=document.querySelector('.stardiv .newstar4');
let star5=document.querySelector('.stardiv .newstar5');

let maindiv =document.querySelector('.put');
let input=document.querySelector('textarea');
let button=document.querySelector('.button');
let s1=document.querySelector('.newstar1');
let s2=document.querySelector('.newstar2');
let s3=document.querySelector('.newstar3');
let s4=document.querySelector('.newstar4');
let s5=document.querySelector('.newstar5');

button.addEventListener('click',(e)=>{
  let div =document.createElement('div');
  let div1 =document.createElement('div');
  let h3 =document.createElement('h3');
  let img1 =document.createElement('img');
  let img2=document.createElement('img');
  let img3 =document.createElement('img');
  let img4 =document.createElement('img');
  let img5 =document.createElement('img');
  let button2 =document.createElement('button');

  let yel='data:image/png;base64,';
  let bal='data:image/png;base64,';
  e.preventDefault();
  let comm=input.value;
  h3.innerText=comm;
  div1.style.border='1px solid black';
  div1.style.margin='20px 0';
  div1.style.padding='10px';
  h3.style.margin='10px';
  img1.style.height='35px';
  img2.style.height='35px';
  img3.style.height='35px';
  img4.style.height='35px';
  img5.style.height='35px';
  button2.innerText='Delete';
  button2.style.backgroundColor='red';
  button2.style.fontSize='20px';
  button2.style.padding='2px';
  

  if(s1.style.color=='orange')
  {
    img1.src=yel;
  }
  else{
    img1.src=bal;
  }

  if(s2.style.color=='orange')
  {
    img2.src=yel;
  }
  else{
    img2.src=bal;
  }

  if(s3.style.color=='orange')
  {
    img3.src=yel;
  }
  else{
    img3.src=bal;
  }

  if(s4.style.color=='orange')
  {
    img4.src=yel;
  }
  else{
    img4.src=bal;
  }

  if(s5.style.color=='orange')
  {
    img5.src=yel;
  }
  else{
    img5.src=bal;
  }
  div.append(img1);
  div.append(img2);
  div.append(img3);
  div.append(img4);
  div.append(img5);

  div1.append(div);
  div1.append(h3);
  div1.append(button2);
  maindiv.append(div1);

  button2.addEventListener('click',()=>{
    div1.remove();
  })
  input.value='';

})


star1.addEventListener('mouseover',()=>{
    if(star1.style.color=='orange')
    {
    star1.style.color='black';
    star2.style.color='black';
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
    }
    else{
      star1.style.color='orange';
    }
    console.log('ppppppp0');
});

star2.addEventListener('mouseover',()=>{
  if(star2.style.color=='orange')
  {
    
    star2.style.color='black';
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
  }
    
});
star3.addEventListener('mouseover',()=>{
  if(star3.style.color=='orange')
  {
    
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
  }
});
star4.addEventListener('mouseover',()=>{
  if(star4.style.color=='orange')
  {
    
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
    star4.style.color='orange';
  }
});
star5.addEventListener('mouseover',()=>{
  if(star5.style.color=='orange')
  {
    
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
    star4.style.color='orange';
    star5.style.color='orange';
  }
});

star1.addEventListener('click',()=>{
    if(star1.style.color=='orange')
    {
    star1.style.color='black';
    star2.style.color='black';
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
    }
    else{
      star1.style.color='orange';
    }
    console.log('ppppppp0');
});

star2.addEventListener('click',()=>{
  if(star2.style.color=='orange')
  {
    
    star2.style.color='black';
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
  }
    
});
star3.addEventListener('click',()=>{
  if(star3.style.color=='orange')
  {
    
    star3.style.color='black';
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
  }
});
star4.addEventListener('click',()=>{
  if(star4.style.color=='orange')
  {
    
    star4.style.color='black';
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
    star4.style.color='orange';
  }
});
star5.addEventListener('click',()=>{
  if(star5.style.color=='orange')
  {
    
    star5.style.color='black';
  }
  else{
    star1.style.color='orange';
    star2.style.color='orange';
    star3.style.color='orange';
    star4.style.color='orange';
    star5.style.color='orange';
  }
});

</script>
</body>
</html>